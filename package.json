{
    "name": "vscode-why-this-file",
    "displayName": "vscode-why-this-file",
    "description": "Why this fie ? (find node module depends on this file)",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.46.0"
    },
    "publisher": "bung87",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscode-why-this-file.why"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-why-this-file.why",
                "title": "Why this fie ? (find node module depends on this file)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "rollup -c rollup.config.ts",
        "lint": "eslint src --ext ts",
        "prettier":"prettier src -w",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "test": "node ./out/test/runTest.js",
        "build": "tsc -p ."
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^19.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.2",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.46.0",
        "@typescript-eslint/eslint-plugin": "^4.28.3",
        "@typescript-eslint/parser": "^4.28.3",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "husky": "^7.0.1",
        "mocha": "^8.1.3",
        "prettier": "^2.3.2",
        "rollup": "^2.53.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    },
    "dependencies": {
        "@ad2302/why-this-file": "^1.0.1"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.com"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test"
        }
    }
}
